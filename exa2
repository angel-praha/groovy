node {
      for (i=0; i<2; i++) {
           stage "Stage #"+i
           print 'Hello, world !'
           if (i==0)
           {
               git "<a href="https://github.com/angel-praha/groovy/blob/master/atleti">https://github.com/Zulaikha12/gitnew.git</a>"
               echo 'Running on Stage #0'
           }
           else {
               build 'Declarative pipeline'
               echo 'Running on Stage #1'
           }
      }
}
